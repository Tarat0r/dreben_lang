Променлива a равна 2;
Променлива b равна 3;
Променлива c равна "Здравей ";
Променлива d равна "свят!";
Променлива e равна Лъжа;

Покажи a плюс b;
Покажи c плюс d;

Покажи (a плюс b плюс 1) делено на 3 по 10;

 
Ако „3 е по-голямо от 1“ {
    Променлива p равна 11;
    Покажи p;
}
Иначе {
    Покажи "Все работает!" плюс " здр";
}


Ако „e“ {
    Покажи "итмо";
}
Иначе {
    Докато b е по-голямо от 0 {
        Покажи "МНОГО";
        b равно b минус 1;
    }
}
